{
    "name": "madmikeyb/sonos-slackbot",
    "description": "Control your SONOS Speaker System through an easy to use Slack Bot",
    "keywords": [
        "madmikeyb",
        "sonos",
        "slack",
        "sonos slack",
        "symfony"
    ],
    "license": "MIT",
    "homepage": "https://github.com/madmikeyb/sonos-slackbot",
    "authors": [
        {
            "name": "Michael Burton",
            "email": "me@mikeylicio.us"
        }
    ],
    "require": {
        "php" : ">=7.0",
        "symfony/console": "^3.3",
        "psr/log": "1.0.0",
        "duncan3dc/sonos": "^2.0",
        "jwilsson/spotify-web-api-php": "^2.0",
        "vlucas/phpdotenv": "^2.4",
        "coderstephen/slack-client": "^0.3.0",
        "illuminate/support": "^5.5"
    },
    "autoload": {
        "psr-4": { 
            "MadMikeyB\\SonosBot\\": "src" 
        }
    },
    "require-dev": {
        "symfony/var-dumper": "^3.3"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
